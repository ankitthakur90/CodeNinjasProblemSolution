// function return vector solution c++

#include <bits/stdc++.h> 
vector<int> swapNumber(int x, int y) {
    x = x^y;
    y = x^y;
    x = x^y;
    vector<int>ans;
    ans.push_back(x);
    ans.push_back(y);
    return ans;
}
